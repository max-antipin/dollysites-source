{
  "$schema": "./node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "7.5.0",
    "generators": {
      "v3.0": {
        "generatorName": "php-symfony",
        "templateDir": "openapi-demo/custom-templates",
        "output": "openapi-demo-output/",
        "inputSpec": "openapi-demo/openapi.yaml",
        "ignoreFileOverride": "openapi-demo/.openapi-generator-ignore",
        "additionalProperties": {
          "srcBasePath": "source/",
          "bundleName": "GeneratedAPI"
        },
        "enablePostProcessFile": true
      }
    }
  }
}
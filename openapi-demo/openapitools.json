{
  "$schema": "https://raw.githubusercontent.com/OpenAPITools/openapi-generator-cli/master/apps/generator-cli/src/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "7.5.0",
    "generators": {
      "v3.0": {
        "generatorName": "php-symfony",
        "templateDir": "openapi-demo/custom-templates",
        "output": "openapi-demo-output-0/",
        "inputSpec": "openapi-demo/openapi.yaml",
        "additionalProperties": {
          "srcBasePath": "source/"
        },
        "enablePostProcessFile": true
      },
      "v3.5": {
        "generatorName": "php-symfony",
        "templateDir": "openapi-demo/custom-templates",
        "output": "openapi-demo-output-1/",
        "inputSpec": "openapi-demo/openapi.yaml",
        "ignoreFileOverride": "openapi-demo/.openapi-generator-ignore",
        "additionalProperties": {
          "srcBasePath": "../src/OpenAPI/",
          "bundleName": "ServerAPI",
          "bundleAlias": "server_api",
          "invokerPackage": "App\\OpenAPI"
        },
        "enablePostProcessFile": true,
        "dryRun": true
      }
    }
  }
}